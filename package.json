    "riteway": "6.2.1",
    "tailwindcss": "^2.0.3",
    "tap-nirvana": "1.1.0",
    "testcafe": "^1.10.1",
    "uuid": "8.3.1",
    "watch": "1.0.2",
    "web-audio-api": "0.2.2"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn -s validate"
    }
  },
  "name": "earlynode-next-starter",
  "private": true,
  "scripts": {
    "cypress": "cypress run --spec src/**/**.functional-test.js",
    "build": "next build",
    "dev": "next dev",
    "format": "echo 'Linting ...' && yarn -s lint --fix && echo 'Lint complete.'",
    "functional-tests:ci": "run-s functional-tests:chrome-desktop:headless functional-tests:chrome-mobile:headless",
    "functional-tests:chrome-desktop:headful": "testcafe 'chrome --allow-insecure-localhost --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' src/**/*.functional-test.js",
    "functional-tests:chrome-desktop:headless": "testcafe 'chrome:headless --allow-insecure-localhost --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' src/**/*.functional-test.js",
    "functional-tests:chrome-mobile:headful": "testcafe 'chrome:emulation:width=320;height=568;mobile=true;orientation=vertical;touch=true  --allow-insecure-localhost --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' src/**/*.functional-test.js",
    "functional-tests:chrome-mobile:headless": "testcafe 'chrome:headless:emulation:width=320;height=568;mobile=true;orientation=vertical;touch=true  --allow-insecure-localhost --use-fake-ui-for-media-stream --use-fake-device-for-media-stream' src/**/*.functional-test.js",
    "functional-tests:firefox-desktop": "testcafe 'firefox:userProfile' src/**/*.functional-test.js",
    "lint": "eslint --ignore-path .gitignore .",
    "start": "next start",
    "unit-tests": "NODE_ENV=test riteway -r @babel/register 'src/**/*.test.js' | tap-nirvana",
    "validate": "yarn -s unit-tests && yarn -s lint && yarn -s cypress",
    "watch": "watch 'clear && run-s unit-tests format' src"
  },
  "version": "0.1.0"
}
